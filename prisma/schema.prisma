generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
}

model Item {
  id        Int    @id @default(autoincrement())
  uuid      String
  name      String
  usedfor   String
  tipstriks String
  mistakes  String
}
